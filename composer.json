{
  "name": "shin1x1/safe-cast",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "shin1x1",
      "email": "shin1x1@gmail.com"
    }
  ],
  "require": {
      "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "friendsofphp/php-cs-fixer": "^3.17",
    "phpstan/phpstan": "^1.10",
    "vimeo/psalm": "^5.12"
  },
  "autoload": {
    "psr-4": {
      "Shin1x1\\SafeCast\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shin1x1\\SafeCast\\Tests\\": "tests/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "scripts": {
    "php-cs-fixer": "php-cs-fixer fix -v --diff",
    "phpstan": "phpstan analyse",
    "psalm": "psalm",
    "phpunit": "phpunit "
  }
}
